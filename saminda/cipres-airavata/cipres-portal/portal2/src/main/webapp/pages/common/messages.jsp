<%@ taglib prefix="s" uri="/struts-tags" %>

<s:if test="%{hasMessages()}">
  <span class="strutsMessage">
    <span class="strutsConfirm">
      <s:actionmessage/>
    </span>
    <span class="strutsError">
      <s:actionerror/>
      <s:fielderror/>
    </span>
  </span>
  <br/>
</s:if>

